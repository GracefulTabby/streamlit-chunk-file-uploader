{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    },
    "black-formatter.args": [
        "--line-length=120"
    ],
    "files.exclude": {
        "**/.DS_Store": true,
        "**/node_modules": true,
        "build": true
    }
}